  "scripts": {
    "start": "react-scripts start",
    "start:local": "cross-env PROXY_URL=http://localhost:3010 react-scripts start",
    "start:screen": "screen -S folk-webapp -c .screenrc",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "reinstall": "rm -rf ./node_module && yarn",
    "predeploy:dev": "yarn build",
    "predeploy:prod": "yarn build",
    "deploy:dev": "aws s3 sync build s3://dev-folk-webapp-static-web",
    "deploy:prod": "aws s3 sync build s3://prod-folk-webapp-static-web",
    "postdeploy:dev": "aws cloudfront create-invalidation --distribution-id E215UYR726ILDS --paths /",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id E16SNX0CEZ21PY --paths /",
    "sls": "sls",
    "prepare": "husky install",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "lint": "eslint --quiet ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "resetMocks": false
  },
  "proxy": "https://dev.folk.knowit.no/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14.4.0",
    "npm": "please-use-yarn",
    "yarn": "^1.22.5"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@nivo/bar": "^0.67.0",
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "@nivo/pie": "^0.67.0",
    "@nivo/radar": "^0.67.0",
    "@nivo/sunburst": "^0.67.0",
    "axios": "^0.25.0",
    "browserslist": "^4.19.1",
    "http-proxy-middleware": "^2.0.2",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "react-virtualized": "^9.22.2",
    "swr": "^1.2.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized": "^9.21.17",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "serverless-plugin-typescript": "^2.1.1"
  },
  "resolutions": {
    "node-fetch": ">=2.6.7",
    "immer": ">=9.0.12",
    "postcss": ">=8.2.13",
    "nth-check": ">=2.0.1"
  }
}
