  "scripts": {
    "start": "react-scripts start",
    "start:local": "cross-env PROXY_URL=http://localhost:3010 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "reinstall": "rm -rf ./node_module && yarn",
    "predeploy:dev": "yarn build",
    "predeploy:prod": "yarn build",
    "deploy:dev": "aws s3 sync build s3://dev-folk-webapp-static-web",
    "deploy:prod": "aws s3 sync build s3://prod-folk-webapp-static-web",
    "postdeploy:dev": "postdeploy && aws cloudfront create-invalidation --distribution-id E215UYR726ILDS --paths /",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id E16SNX0CEZ21PY --paths /",
    "sls": "sls",
    "prepare": "husky install",
    "format": "run -T prettier --write ./src",
    "format:check": "run -T prettier --check ./src",
    "lint": "run -T eslint .",
    "lint:fix": "run -T eslint --fix ./src/**/*.{ts,tsx}"
  },
  "prettier": "@folk/prettier-config",
  "proxy": "https://dev.folk.knowit.no/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14.4.0",
    "npm": "please-use-yarn"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@nivo/bar": "^0.67.0",
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "@nivo/pie": "^0.67.0",
    "@nivo/radar": "^0.67.0",
    "@nivo/sunburst": "^0.67.0",
    "axios": "^0.26.1",
    "browserslist": "^4.20.2",
    "http-proxy-middleware": "^2.0.4",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "react-virtualized": "^9.22.2",
    "swr": "^1.2.2",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@folk/common": "workspace:^",
    "@folk/prettier-config": "workspace:^",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized": "^9.21.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.0"
  }
}
